  y[i]:=round(B*yy[i]): y[i+1]:=round(B*yy[i+1]):
  v[i]:=round(B*(yy[i+1]-yy[i])/(t[i+1]-t[i])): v[i+1]:=round(B*(yy[i+2]-yy[i+1])/(t[i+2]-t[i+1])):

