#Сон
#Вычисляем v_rrh и v_nrrh
v_rrh:='v_rrh':
v_nrrh:='v_nrrh':

NNN:='NNN':
NNNNN:='NNNNN':
NNN:=nops(rvn):
i:='i': NNNNN:=sum(rvn[i][3],i=1..NNN): i:='i':
v_rrh:=0:
v_nrrh:=0:
i:='i':
for i from 1 to NNN do
 if abs(rvn[i][2])<=15 then
  v_rrh:=v_rrh+rvn[i][3]*abs(rvn[i][2])/NNNNN:
 else
  v_nrrh:=v_nrrh+rvn[i][3]*abs(rvn[i][2])/NNNNN:
 end if:
end do:
i:='i':
NNN:='NNN':
NNNNN:='NNNNN':

v_rrh:=evalf(v_rrh);
v_nrrh:=evalf(v_nrrh);

#Бодрствование
rvn:='rvn':
rvn:=rvn_bodrstvovanie[ii]:

#Вычисляем v_rrh и v_nrrh
v_rrh:='v_rrh':
v_nrrh:='v_nrrh':

NNN:='NNN':
NNNNN:='NNNNN':
NNN:=nops(rvn):
i:='i': NNNNN:=sum(rvn[i][3],i=1..NNN): i:='i':
v_rrh:=0:
v_nrrh:=0:
i:='i':
for i from 1 to NNN do
 if abs(rvn[i][2])<=15 then
  v_rrh:=v_rrh+rvn[i][3]*abs(rvn[i][2])/NNNNN:
 else
  v_nrrh:=v_nrrh+rvn[i][3]*abs(rvn[i][2])/NNNNN:
 end if:
end do:
i:='i':
NNN:='NNN':
NNNNN:='NNNNN':

v_rrh:=evalf(v_rrh);
v_nrrh:=evalf(v_nrrh);
